{
  "name": "greflash",
  "version": "1.0.0",
  "description": "A flashcard app for GRE preparation",
  "main": "Main.js",
  "bin": {
    "greflash": "./Main.js"
  },
  "scripts": {
    "start": "node Main.js"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "sqlite": "^5.1.1",
    "sqlite3": "https://github.com/mapbox/node-sqlite3/tarball/master"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "Database/Data.db"
    ],
    "scripts": [
